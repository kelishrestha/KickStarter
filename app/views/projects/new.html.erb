<h1>Project Registration Form</h1>



<%= form_for @project do |f| %>
	<%= f.label :title, 'Project title'   %>
	<%= f.text_field :title%>

	<%= f.label :description %>
	<%= f.text_field :description%>

	<%= f.label :abstract %>
	<%= f.text_field :abstract%>

	<%= f.label :budget %>
	<%= f.text_field :budget%>

	<%= f.submit 'Add'%>
<%end %>